							<h3>College Saint Paul&Pierre</h3>
							<span>Ministere des enseignements secondaire</span>
							<span>Adresse 123, Cameroun</span>
							<span>Ph. 001-44556655: teststudenttracer@gmail.com</span>
							<h4>Annee scolaire: 2019-2020</h4>
							<h4>Sequence: </h4>
						</div>
					</div>
					<div class="col-md-4">
						<div class="text-center">
							<h3>LOGO</h3>
						</div>
					</div>
				</div>
				<!-- ID -->
				<div class="row">
					<div class="col-md-2 offset-md-7">
						<div class="input-group mb-3" style="display: flex;">
							<div class="input-group-prepend">
								<span class="input-group-text" id="inputGroup-sizing-sm">Identifiant.</span>
							</div>
							<div class="form-control w-25">
							    <span aria-label="Small" aria-describedby="inputGroup-sizing-sm">1</span>
							</div>
						</div>
					</div>
				</div>
				<!-- INFOS -->
				<div class="container">
					<div class="row">
						<div class="col-md-10 offset-md-1">
							Nom de l'eleve: <u class="text-center font-weight-bold">tekeu ange</u>
						</div>
					</div>	
					<div class="row">
						<div class="col-md-11 offset-md-1">
							Prenom de l'eleve: <u class="text-center font-weight-bold">tekeu ange</u>
						</div>
					</div>
					<div class="row">
						<div class="col-md-11 offset-md-1">
							Nom du Tuteur: <u class="text-center font-weight-bold">tekeu ange</u>
						</div>
					</div>
					<div class="row">
						<div class="col-md-11 offset-md-1">
							Date de naissance: <u class="text-center font-weight-bold">tekeu ange</u>
						</div>
					</div>
					<div class="row">
						<div class="col-md-11 offset-md-1">
							Classe / Sequence: <u class="text-center font-weight-bold">tekeu ange</u>
						</div>
					</div>
				</div>
				<!-- TABLE -->
				<div class="container">
					<div class="col-lg-9 offset-lg-1" style="padding: 0px; margin-top: 1rem;">
						<table class="table table-striped">
						  <thead>
						    <tr>
						      <th scope="col">MATIERES</th>
						      <th scope="col">Notes</th>
						      <th scope="col">Coef</th>
						      <th scope="col">Enseignant</th>
						      <th scope="col" colspan="5">Appreciations</th> 
						      <th scope="col">Mention</th>
						      <th scope="col">Rank</th>
						    </tr>
						  </thead>
						  <tbody>
						    <tr>
						      <th scope="row">Mathematique</th>
						      <td>15</td>
						      <td>4</td>
						      <td scope="col">Mr IDRISS</td>
						      <td colspan="5">Bon eleve mais peux mieux faire!</td>
						      <td>B</td>
						      <td>2</td>
						    </tr>
						  </tbody>
						</table>
					</div>
				</div>
				<!-- RESULTATS -->
				<div class="container">
					<div class="row text-center">
						<!-- MOYENNE -->
						<div class="col-md-2 offset-md-4">
							<div class="input-group mb-3" style="display: flex;">
							    <div class="input-group-prepend">
							    	<span class="input-group-text" id="inputGroup-sizing-sm">Moyenne</span>
							    </div>
							    <div class="form-control w-25">
							    	<span aria-label="Small" aria-describedby="inputGroup-sizing-sm">/20</span>
								</div>
							</div>
						</div>
						<!-- MENTION -->
						<div class="col-md-2">
							<div class="input-group mb-3" style="display: flex;">
							    <div class="input-group-prepend">
							    	<span class="input-group-text" id="inputGroup-sizing-sm">Mention</span>
							    </div>
							    <div class="form-control w-25">
							    	<span aria-label="Small" aria-describedby="inputGroup-sizing-sm">AB</span>
								</div>
							</div>
						</div>
						<!-- RANK -->
						<div class="col-md-2">
							<div class="input-group mb-3" style="display: flex;">
							    <div class="input-group-prepend">
							    	<span class="input-group-text" id="inputGroup-sizing-sm">Rank</span>
							    </div>
							    <div class="form-control w-25">
							    	<span aria-label="Small" aria-describedby="inputGroup-sizing-sm">1</span>
								</div>
							</div>
						</div>
					</div>
				</div>
			</main>
		</div>
	</div>  
</body>
</html>